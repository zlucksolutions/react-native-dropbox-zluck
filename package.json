{
  "name": "react-native-dropbox-zluck",
  "version": "1.0.0",
  "description": "React native dropbox upload library, for small and large files",
  "main": "src/index.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/zlucksolutions/react-native-dropbox-zluck.git"
  },
  "keywords": [
    "react-native",
    "dropbox",
    "large file upload",
    "small file upload",
    "chunk upload"
  ],
  "scripts": {
    "test": "jest",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "fixcode": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "android-clean": "cd android && ./gradlew clean",
    "apk": "cd android && ./gradlew assembleRelease",
    "bundle": "cd android && ./gradlew bundleRelease",
    "clean": "rm -rf node_modules ios/Pods && yarn && npx pod-install ios"
  },
  "author": "Zluck Solutions",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-native": "0.71.2",
    "react-native-app-auth": "^6.4.3",
    "react-native-fs": "^2.20.0",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.177",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.71.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "jsc-android": "^250230.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
